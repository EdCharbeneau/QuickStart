<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Scripts.Render("~/bundles/modernizr")

	<!--This bundle was moved by the Telerik VS Extensions for compatibility reasons-->
	@Scripts.Render("~/bundles/jquery")
    	<link href="http://cdn.kendostatic.com/2015.3.930/styles/kendo.common-nova.min.css" rel="stylesheet" type="text/css" />
	<link href="http://cdn.kendostatic.com/2015.3.930/styles/kendo.mobile.all.min.css" rel="stylesheet" type="text/css" />
	<link href="http://cdn.kendostatic.com/2015.3.930/styles/kendo.dataviz.min.css" rel="stylesheet" type="text/css" />
	<link href="http://cdn.kendostatic.com/2015.3.930/styles/kendo.nova.min.css" rel="stylesheet" type="text/css" />
	<link href="http://cdn.kendostatic.com/2015.3.930/styles/kendo.dataviz.nova.min.css" rel="stylesheet" type="text/css" />
	<link href="http://cdn.kendostatic.com/2015.2.902/styles/kendo.dataviz.bootstrap.min.css" rel="stylesheet" type="text/css" />
	<script src="http://cdn.kendostatic.com/2015.3.930/js/jquery.min.js"></script>
	<script src="http://cdn.kendostatic.com/2015.3.930/js/jszip.min.js"></script>
	<script src="http://cdn.kendostatic.com/2015.3.930/js/kendo.all.min.js"></script>
	<script src="http://cdn.kendostatic.com/2015.3.930/js/kendo.aspnetmvc.min.js"></script>
@Styles.Render("~/Content/css")
	<script src="@Url.Content("~/Scripts/kendo.modernizr.custom.js")"></script>
</head>
<body>
    <div class="container-fluid body-content">
        @RenderBody()
    </div>

    
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
